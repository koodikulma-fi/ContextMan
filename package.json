  "keywords": [
    "ContextMan",
    "context-man",
    "context man",
    "context",
    "man",
    "manager",
    "context tools",
    "web tools",
    "library"
  ],
  "homepage": "https://github.com/koodikulma-fi/context-man",
  "author": "Mikael Saur√©n",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koodikulma-fi/context-man.git"
  },
  "module": "./dist/ContextMan.module.js",
  "main": "./dist/ContextMan.js",
  "types": "./dist/ContextMan.d.ts",
